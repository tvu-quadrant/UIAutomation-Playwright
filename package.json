{
  "name": "uiautomation-playwright",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:service": "playwright test --config=playwright.service.config.js",
    "test:service:headed": "playwright test --config=playwright.service.config.js --headed",
    "test:service:create-bridge": "playwright test tests/create-bridge.spec.js --config=playwright.service.config.js",
    "report": "playwright show-report",
    "save-auth": "node ./scripts/save-ms-auth.js",
    "test:debug": "powershell -NoProfile -Command \"npx playwright test --debug\"",
    "func:start": "powershell -NoProfile -Command \"Set-Location azure-function-app; func start\""
  },
  "devDependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/playwright": "^1.1.1",
    "@playwright/test": "^1.57.0"
  },
  "dependencies": {
    "@azure/keyvault-secrets": "^4.10.0",
    "dotenv": "^17.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
