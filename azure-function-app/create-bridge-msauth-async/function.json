{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"],
      "route": "create-bridge-msauth-async"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "runQueueMessage",
      "queueName": "pw-runs",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
